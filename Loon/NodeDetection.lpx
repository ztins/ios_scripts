LD = switch, true, false, tag = 地理位置查询
NUD = switch, true, false, tag = 节点解锁查询

[Script]
generic script-path=https://kelee.one/Resource/JavaScript/Node_detection_tool/NetworkEntryAndExitQueries.js, timeout=10, tag=入口落地查询,img-url=globe.asia.australia.system, enable={NEAEQ}
